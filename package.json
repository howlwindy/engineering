{
  "name": "harder_better_faster_stronger_engineering",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite serve --mode development",
    "test": "",
    "test:unit": "jest",
    "test:e2e": "",
    "prod": "vite serve --mode production",
    "build:dev": "vue-tsc --noEmit && vite build --mode development",
    "build:test": "vue-tsc --noEmit && vite build --mode test",
    "build:prod": "vue-tsc --noEmit && vite build --mode production",
    "eslint": "eslint . --ext .ts,.vue --fix",
    "stylelint": "stylelint \"**/*.{scss,vue}\" --color --fix",
    "prettier": "prettier . --write",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "**/*.{html,json,yaml,md,ts,scss,vue}": [
      "prettier . --write"
    ],
    "**/*.{ts,vue}": [
      "eslint . --ext .ts,.vue --fix"
    ],
    "**/*.{scss,vue}": [
      "stylelint \"**/*.{scss,vue}\" --color --fix"
    ]
  },
  "dependencies": {
    "@vueuse/components": "^7.5.5",
    "@vueuse/core": "^7.5.3",
    "@vueuse/integrations": "^7.5.5",
    "@vueuse/motion": "^2.0.0-beta.9",
    "@vueuse/rxjs": "^7.5.5",
    "d3": "^7.3.0",
    "fuse": "^0.4.0",
    "pinia": "^2.0.11",
    "rxjs": "^7.5.2",
    "vue": "^3.2.26",
    "vue-i18n": "^9.2.0-beta.30",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.11.19",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vitejs/plugin-vue": "^1.10.2",
    "@vue/test-utils": "^2.0.0-rc.18",
    "autoprefixer": "^10.4.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-vue": "^8.3.0",
    "husky": "^7.0.4",
    "jest": "^26.6.3",
    "lint-staged": "^11.2.6",
    "postcss": "^8.4.5",
    "postcss-html": "^1.3.0",
    "postcss-import": "^14.0.2",
    "postcss-reporter": "^7.0.5",
    "postcss-sorting": "^7.0.1",
    "prettier": "^2.5.1",
    "sass": "^1.48.0",
    "stylelint": "^14.2.0",
    "stylelint-config-hudochenkov": "^6.0.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-config-recommended-vue": "^1.1.0",
    "stylelint-config-standard": "^23.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.1.0",
    "tailwindcss": "^3.0.13",
    "ts-jest": "^26.5.6",
    "typescript": "^4.5.4",
    "vite": "^2.7.12",
    "vue-eslint-parser": "^8.0.1",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-tsc": "^0.3.0"
  }
}
